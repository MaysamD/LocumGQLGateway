{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Quartz": "Warning"
    }
  },
  "Quartz": {
    "Jobs": [
      {
        "Name": "Job1",
        "CronSchedule": "0/10 * * * * ?",
        // every 10 seconds
        "Description": "Runs every 10 seconds"
      },
      {
        "Name": "Job2",
        "CronSchedule": "0 * * ? * *",
        // every minute
        "Description": "Runs every minute"
      }
    ]
  }
}
